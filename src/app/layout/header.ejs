<!-- Header -->
<header class='sh-ui' itemscope='itemscope' itemtype='https://schema.org/WPHeader'>
<b:class cond='data:skin.vars.navFixed' name='nav-fixed'/>
<b:class cond='data:skin.vars.layoutBox' name='m-auto'/>
<b:class cond='data:skin.vars.navColor' name='bg-header color-white'/>
<b:class cond='data:skin.vars.navUi' name='dk:bg-white dk:color-black'/>
<div class='container menu flex align-items-center justify-between'>
<div class='left'>
<div class='logo'>
<b:section id='header'>
  <b:widget id='Header1' locked='false' title='Asap theme (Header)' type='Header' version='2' visible='true'>
    <b:widget-settings>
      <b:widget-setting name='displayUrl'/>
      <b:widget-setting name='displayHeight'>0</b:widget-setting>
      <b:widget-setting name='sectionWidth'>-1</b:widget-setting>
      <b:widget-setting name='useImage'>false</b:widget-setting>
      <b:widget-setting name='shrinkToFit'>false</b:widget-setting>
      <b:widget-setting name='imagePlacement'>BEHIND</b:widget-setting>
      <b:widget-setting name='displayWidth'>0</b:widget-setting>
    </b:widget-settings>
    <b:includable id='main' var='this'>
<b:include data='{[&quot;id&quot;],[&quot;data-version&quot;]}' name='@attr'/>
<b:include expr:name='&quot;widget:&quot; + data:widget.type'/>
</b:includable>
    <b:includable id='behindImageStyle'/>
    <b:includable id='content'/>
    <b:includable id='description'/>
    <b:includable id='image'/>
    <b:includable id='title'/>
  </b:widget>
</b:section>
</div>
</div>
<div class='right flex align-items-center gap-md'>
<input id='offcanvas' type='checkbox'/>
<div class='menu-offcanvas'>
<div id='menu-header'>
<b:section id='menu'>
  <b:widget id='PageList1' locked='false' title='' type='PageList' version='2' visible='true'>
    <b:widget-settings>
      <b:widget-setting name='pageListJson'><![CDATA[{"link1":{"href":"#","position":1,"title":"Categorias"},"link0":{"href":"https://asap-demo.blogspot.com/2024/08/la-mejor-plantilla-wordpress-para-seo.html","position":0,"title":"Wordpress"},"link2":{"href":"https://asap-demo.blogspot.com/2024/08/totalmente-adaptada-para-dispositivos.html","position":2,"title":"__Resposive"}}]]></b:widget-setting>
      <b:widget-setting name='homeTitle'>Home</b:widget-setting>
    </b:widget-settings>
    <b:includable id='main' var='this'>
<b:include name='widget-title'/>
<b:include name='content'/>
</b:includable>
    <b:includable id='content'>
<div class='widget-content'>
<b:include expr:name='&quot;widget:&quot; + data:widget.type'/>
</div>
</b:includable>
    <b:includable id='overflowButton'/>
    <b:includable id='overflowablePageList'/>
    <b:includable id='pageLink'/>
    <b:includable id='pageList'/>
  </b:widget>
</b:section>
</div>
</div>
<!-- SearchForm -->
<input id='offsearch' type='checkbox'/>
<form class='search-offcanvas' expr:action='data:blog.searchUrl'>
<input class='w-full' expr:value='data:view.isSearch ? data:view.search.query.escaped : &quot;&quot;' name='q' placeholder='Buscar en el blog'/>
</form>
<!--  Modo oscuro -->
<div class='theme-switch-wrapper'>
<label class='theme-switch' for='checkbox'>
<input id='checkbox' type='checkbox'/>
<div class='slider'>
<div class='color-white'>
<b:if cond='data:skin.vars.navUi'>
<b:attr name='class' value='dk-color-dark'/>
</b:if>
<svg class='i-sm  feather-sun i-moon' viewBox='0 0 24 24'><path d='M21 14a9 9 0 1 1-9-11 7 7 0 0 0 9 11z'/></svg>
<svg class='i-sm feather-moon i-sun' viewBox='0 0 24 24'><circle cx='12' cy='12' r='5'/><path d='M12 1v2m0 18v2M4.2 4.2l1.4 1.4m12.8 12.8 1.4 1.4M1 12h2m18 0h2M4.2 19.8l1.4-1.4M18.4 5.6l1.4-1.4'/> </svg>
</div>
</div>
</label>
</div>
<!-- Icon Search -->
<label class='offsearch' for='offsearch'>
<i data-i='search'/>
</label>
<!-- Icon menu -->
<label class='offcanvas' for='offcanvas'>
<i data-i='bars'/>
</label>
</div>
</div>
</header>